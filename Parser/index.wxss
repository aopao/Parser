.navigator-hover {
  opacity: 0.7;
  text-decoration: underline;
}
.b_rich, .strong_rich, .del_rich, .ins_rich, .em_rich, .i_rich, .sub_rich, .sup_rich, .q_rich, .span_rich {
  display: inline-block;
}
.b, .strong {
  font-weight: bold;
  display: inline-block;
}
.del {
  text-decoration: line-through;
  display: inline-block;
}
.ins {
  text-decoration: underline;
  display: inline-block;
}
.em, .i {
  font-style: italic;
  display: inline-block;
}
.sub, .sup, .q, .span {
  display: inline-block;
}
.q::before {
  content: '"';
}
.q::after {
  content: '"';
}